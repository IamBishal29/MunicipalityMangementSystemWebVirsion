@model FinalEntity.Complain

@{
    ViewBag.Title = "CreateComplain";
    Layout = "~/Views/Shared/_ProfileLayout.cshtml";
}


@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Complain</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.ComplainMsg)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ComplainMsg)
            @Html.ValidationMessageFor(model => model.ComplainMsg)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Other)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Other)
            @Html.ValidationMessageFor(model => model.Other)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.UserId)
        </div>
        <div class="editor-field">
             @Html.TextBoxFor(model => model.UserId,new {@Value=Session["ID"] })
            @Html.ValidationMessageFor(model => model.UserId)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

